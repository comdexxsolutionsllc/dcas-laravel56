{
  "name": "fiberhopllc_dcas",
  "description": "FiberHop LLC - DCAS - Datacenter Automation Suite",
  "keywords": [
    "framework",
    "laravel",
    "dcas",
    "web hosting",
    "datacenter",
    "fiberhop"
  ],
  "license": "Closed Source",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "askedio/laravel-soft-cascade": "^5.6",
    "bpocallaghan/alert": "^1.1",
    "bpocallaghan/impersonate": "^1.0",
    "bpocallaghan/notify": "^2.1",
    "bpocallaghan/sluggable": "^1.2",
    "campaignmonitor/createsend-php": "^5.1",
    "consoletvs/bootpack": "^1.1",
    "davejamesmiller/laravel-breadcrumbs": "5.x",
    "delatbabel/elocryptfive": "^1.5",
    "doctrine/dbal": "^2.6",
    "dompdf/dompdf": "*",
    "erusev/parsedown-extra": "^0.7.1",
    "fewagency/laravel-reformulator": "dev-master",
    "fideloper/proxy": "~4.0",
    "geoip/geoip": "^1.17",
    "google/recaptcha": "~1.1",
    "graze/telnet-client": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "h4cc/wkhtmltoimage-amd64": "0.12.x",
    "h4cc/wkhtmltopdf-amd64": "0.12.x",
    "hyn/multi-tenant": "^5.2",
    "kalnoy/nestedset": "^4.2",
    "kitetail/zttp": "^0.3.0",
    "laracasts/flash": "^3.0",
    "laravel-frontend-presets/tailwindcss": "^0.4.3",
    "laravel-shield/github": "^1.0",
    "laravel-shield/mailgun": "^1.0",
    "laravel-shield/shield": "^1.0",
    "laravel-shield/stripe": "^1.0",
    "laravel/cashier": "~7.0",
    "laravel/envoy": "^1.4",
    "laravel/framework": "5.6.*",
    "laravel/horizon": "^1.2",
    "laravel/passport": "^5.0",
    "laravel/scout": "^4.0",
    "laravelcollective/html": "^5.6",
    "league/pipeline": "^0.3.0",
    "maatwebsite/excel": "~2.1.0",
    "maddhatter/laravel-fullcalendar": "^1.3",
    "moontoast/math": "*",
    "nunomaduro/laravel-console-menu": "^1.0",
    "nwidart/laravel-modules": "^2.7",
    "nztim/mailchimp": "^3.7",
    "predis/predis": "^1.1",
    "propaganistas/laravel-phone": "^4.0",
    "pusher/pusher-php-server": "~3.0",
    "silber/bouncer": "v1.0.0-rc.1",
    "silber/page-cache": "dev-master",
    "snipe/banbuilder": "^2.2",
    "softonic/laravel-middleware-request-id": "^1.0",
    "spatie/array-to-xml": "^2.5",
    "spatie/laravel-analytics": "^3.6",
    "spatie/laravel-medialibrary": "^7.0.0",
    "spatie/laravel-robots-middleware": "^1.0",
    "stevenmaguire/laravel-middleware-csp": "^0.1.1",
    "stripe/stripe-php": "*",
    "symfony/dom-crawler": "^4.1",
    "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
    "todstoychev/calendar-events": "^1.0",
    "yajra/laravel-datatables": "^1.0",
    "yajra/laravel-datatables-oracle": "~8.0"
  },
  "require-dev": {
    "ahuggins/open-on-make": "^0.0.16",
    "ajthinking/tinx": "^2.0",
    "barryvdh/laravel-debugbar": "^3.1",
    "barryvdh/laravel-ide-helper": "^2.4",
    "beyondcode/laravel-dump-server": "^1.1",
    "crestapps/laravel-code-generator": "*",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "jlapp/swaggervel": "dev-master",
    "laracasts/generators": "^1.1",
    "laravel/dusk": "^3.0",
    "laravel/tinker": "~1.0",
    "mockery/mockery": "~1.0",
    "mtrajano/laravel-swagger": "^0.2.0",
    "nunomaduro/collision": "~1.1",
    "phpmd/phpmd": "^2.6",
    "phpspec/phpspec": "^4.3",
    "phpunit/phpunit": "~7.0",
    "swagger-api/swagger-ui": "^3.13"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "DCAS\\": "app/DCAS",
      "Modules\\": "Modules/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "tests/helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan view:clear",
      "@php artisan cache:clear",
      "@composer dump-autoload"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
